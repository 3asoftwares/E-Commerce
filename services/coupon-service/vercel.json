{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "installCommand": "cd ../.. && yarn install && yarn build:types && yarn build:utils",
  "buildCommand": "yarn build:vercel",
  "functions": {
    "api/index.js": {
      "includeFiles": ".vercel-build/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
